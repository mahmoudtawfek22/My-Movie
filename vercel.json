{
  "version": 2,
  "builds": [
    {
      "src": "dist/Mymovie/server/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/Mymovie/browser/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/browser/server/index.server.html"
    }
  ]
}
