{
  "version": 3,
  "builds": [
    { "src": "dist/browser/**", "use": "@vercel/static" },
    { "src": "dist/server/server.mjs", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/browser/assets/$1"
    },
    {
      "src": "/.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot)$",
      "dest": "/browser/$0"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.mjs"
    }
  ]
}
